import java.util.Scanner;
class Fact
{
    public static void main(String arg[])
    {
        
        Scanner input=new Scanner(System.in);
        int n=input.nextInt();
        int fact=1;
        for(int i=1;i<=n;i++)
        {
            fact=fact*i;
        }
        System.out.println("Factorial is : "+fact);
    }
}
